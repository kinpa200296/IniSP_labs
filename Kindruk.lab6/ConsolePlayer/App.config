<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="RefreshRate" value="50"/>
    <add key="ConsoleBufferWidth" value="150"/>
    <add key="ConsoleBufferHeight" value="50"/>
    <add key="ConsoleWindowWidth" value="150"/>
